# Event Kit Docker Environment Variables
# Copy this file to .env and update with your values

# Application Insights (optional)
APP_INSIGHTS_CONNECTION_STRING=

# API Authentication (optional)
API_TOKEN=

# Microsoft Graph API (optional - required for graph mode)
GRAPH_TENANT_ID=
GRAPH_CLIENT_ID=
GRAPH_CLIENT_SECRET=
GRAPH_USER_ID=

# Internal port (default: 8011)
PORT=8011
