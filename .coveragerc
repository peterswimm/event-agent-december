[run]
source = .
omit =
    tests/*
    .venv/*
    __pycache__/*
    setup.py
    */__pycache__/*
    */tests/*
    */migrations/*
    .pytest_cache/*
    eventkit.egg-info/*
    adapters/directline_bot.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
    if 0:
    if False:

[html]
directory = htmlcov
